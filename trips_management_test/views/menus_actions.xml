<odoo>
    <!--ACCIONES DE VENTANA-->
    <!--Sourcing-->
    <record model="ir.actions.act_window" id="action_list_sourcing">
        <field name="name">trip.abastecimientos</field>
        <field name="res_model">trip.sourcing</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!--Section-->
    <record model="ir.actions.act_window" id="action_list_section">
        <field name="name">trip.tramos</field>
        <field name="res_model">trip.section</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!--Trip-->
    <record model="ir.actions.act_window" id="action_list_trip">
        <field name="name">trip.viajes</field>
        <field name="res_model">trip.trip</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!--Driver-->
    <record model="ir.actions.act_window" id="action_list_driver">
        <field name="name">trip.conductores</field>
        <field name="res_model">trip.driver</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!--Itinerary-->
    <record model="ir.actions.act_window" id="action_list_itinerary">
        <field name="name">trip.itinerarios</field>
        <field name="res_model">trip.itinerary</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!--Stork-->
    <record model="ir.actions.act_window" id="action_list_stork">
        <field name="name">trip.ciguenias</field>
        <field name="res_model">trip.stork</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!--CREAMOS MENUS-->
    <menuitem id="trip_main_menu"
              name="Gestion de Viajes"/>
    <menuitem id="trip_main_menu_register_viajes"
              name="Viajes"
              parent="trip_main_menu"
              action="action_list_trip"/>

