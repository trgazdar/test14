<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="l10n_pe_edi_picking_number_view_form" model="ir.ui.view">
        <field name="name">Picking Number</field>
        <field name="model">l10n_pe_edi.picking.number</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="invoice_id" invisible="True"/>
                            <field name="partner_id" readonly="True" invisible="True"/>
                            <field name="type" required="True"/>
                            <field name="name" required="True"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>