<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record model="ir.rule" id="ple_8">
        <field name="name">ple 8 multi-company</field>
        <field name="model_id" search="[('model','=','ple.report.purchase')]" model="ir.model"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>
    <record model="ir.rule" id="ple_14">
        <field name="name">ple 14 multi-company</field>
        <field name="model_id" search="[('model','=','ple.report.sale')]" model="ir.model"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>
</odoo>