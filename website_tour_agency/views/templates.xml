<odoo>
    <template id="star_card_travel">
        <input type="hidden" name="val_integer" value="0.0" />
        <t t-set="empty_star" t-value="5 - int(val_integer)" />
        <div class="o_website_rating_static">
            <t t-foreach="range(0, int(val_integer))" t-as="num">
                <i class="fa fa-star"></i>
            </t>
            <t t-foreach="range(0, empty_star)" t-as="num">
                <i class="fa fa-star-o"></i>
            </t>
        </div>
    </template>
    <template id="contactus_default_inherit" inherit_id="website.contactus" name="Contactus Inherited">
        <xpath expr="//div[@id='wrap']" position="before">
            <div class="oe_structure" />
            <section class="s_cover parallax s_parallax_is_fixed bg-black-50 pt96 pb96" data-scroll-background-ratio="1" style="background-image: none;">
                <span class="s_parallax_bg oe_img_bg oe_custom_bg" style="background-image: url('/website_tour_agency/static/src/img/contact_header.jpg'); background-position: 50% 0;" />
                <div class="container">
                    <div class="row">
                        <div class="col-12 s_title" data-name="Title">
                            <h1 class="s_title_thin trans_text" style="font-size: 62px; text-align: center;">Let's talk !</h1>
                        </div>
                        <div class="col-12 s_text pt16 pb16" data-name="Text">
                            <p class="lead" style="text-align: center;">Write one or two paragraphs describing your product, services or a specific feature.<br /> To be successful your content needs to be useful to your readers.</p>
                        </div>
                        <div class="col-12 s_btn text-center pt16 pb16" data-name="Buttons">
                            <a href="/aboutus" class="btn btn-delta rounded-circle o_default_snippet_text">About us</a>
                            <a href="/contactus" class="btn btn-primary rounded-circle o_default_snippet_text">Contact us</a>
                        </div>
                    </div>
                </div>
            </section>
            <div class="oe_structure" />
        </xpath>
    </template>
